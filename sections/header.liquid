 .horizontal-mega-menu li.buddha-menu-item.mega-hover>ul.mm-submenu, .horizontal-mega-menu li.buddha-menu-item:hover>ul.mm-submenu,
 .horizontal-mega-menu ul.mm-submenu.tabbed>li>ul.mm-submenu,
 .horizontal-mega-menu ul.mm-submenu, .horizontal-mega-menu>li>ul.mm-submenu.tabbed>li>ul.mm-submenu li,
