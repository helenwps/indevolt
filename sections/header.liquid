<style>
  header-drawer {
    justify-self: start;
    margin-left: -1.2rem;
  }

  {%- if section.settings.sticky_header_type == 'reduce-logo-size' -%}
    .scrolled-past-header .header__heading-logo-wrapper {
      width: 75%;
    }
  {%- endif -%}

  {%- if section.settings.menu_type_desktop != "drawer" -%}
    @media screen and (min-width: 990px) {
      header-drawer {
        display: none;
      }
    }
  {%- endif -%}

  .menu-drawer-container {
    display: flex;
  }

  .list-menu {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  .list-menu--inline {
    display: inline-flex;
    flex-wrap: wrap;
  }

  summary.list-menu__item {
    padding-right: 2.7rem;
  }

  .list-menu__item {
    display: flex;
    align-items: center;
    line-height: calc(1 + 0.3 / var(--font-body-scale));
  }

  .list-menu__item--link {
    text-decoration: none;
    padding-bottom: 1rem;
    padding-top: 1rem;
    line-height: calc(1 + 0.8 / var(--font-body-scale));
  }

  @media screen and (min-width: 750px) {
    .list-menu__item--link {
      padding-bottom: 0.5rem;
      padding-top: 0.5rem;
    }
  }

  
  /* 导航栏基础样式 */
  .main-nav {
    width:44px;
    height:44px;
    display: flex;
    list-style: none;
    margin: 0;
    padding: 0;
    /* background: #f8f8f8; */
  }
  
  .main-nav > li {
    position: relative;
    padding: 8px 15px;
  }
  
  .main-nav a {
    color: #333;
    text-decoration: none;
    font-weight: 500;
  }
  
  /* 下拉菜单样式 */
  .nav-dropdown {
    display: none;
    position: absolute;
    top: 100%;
    left: 0;
    background: white;
    min-width: 200px;
    box-shadow: 0 5px 15px rgba(0,0,0,0.1);
    border-radius: 4px;
    z-index: 1000;
    padding: 10px 0;
  }

   
@media (max-width: 768px) {
  .nav-dropdown {
    display: none;
    position: absolute;
    top: 100%;
    left: -70px;
    background: white;
    min-width: 150px;
    box-shadow: 0 5px 15px rgba(0,0,0,0.1);
    border-radius: 4px;
    z-index: 1000;
    padding: 10px 0;
  }
}
  
  .nav-dropdown li {
    padding: 10px 20px;
    white-space: nowrap;
    
  }
  
  .nav-dropdown li:hover {
    background: #f5f5f5;
  }
  
  /* 悬停显示下拉菜单 */
   .has-dropdown:hover .nav-dropdown {
    display: block;
    animation: fadeIn 0.3s ease;
  }
  
  @keyframes fadeIn {
    from { opacity: 0; transform: translateY(10px); }
    to { opacity: 1; transform: translateY(0); }
  }  
  
  /* 指示箭头 */
  .has-dropdown > a:after {
    content: "▼";
    font-size: 10px;
    margin-left: 5px;
  }
  
 .main-nav, 
.main-nav ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

.main-nav li {
  list-style-type: none; /* 双重保障 */
  list-style-image: none; /* 防止图片标记 */
  list-style-position: outside; /* 标准位置 */
}

  .country-flag {
  width: 24px;
  height: 18px;
  margin-right: 12px;
  border: 1px solid #e0e0e0;
     align-self: center; /* 确保国旗自身垂直居中 */
}
  .nav_li_items {
  display: flex;
  align-items: center; /* 确保国旗和文字垂直居中 */
  /* 其他样式保持不变 */
}

   .header__icon--cart{
   margin-right: 0px !important;
 }



